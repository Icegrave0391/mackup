# Read more about SSH config files: https://linux.die.net/man/5/ssh_config
# Global

Host GitHub
    Hostname github.com
    User git
    PreferredAuthentications publickey
    IdentityFile /Users/chuqi/.ssh/id_rsa

Host kent
    Hostname kentridge.d2.comp.nus.edu.sg
    User chuqi
    IdentityFile /Users/chuqi/.ssh/id_rsa_ida

Host mypc
    Hostname 172.26.187.19
    User chuqiz
    IdentityFile /Users/chuqi/.ssh/id_rsa_mypc

Host kent-mypc
    Hostname 172.26.187.19
    User chuqiz
    ProxyJump kent
    IdentityFile /Users/chuqi/.ssh/id_rsa_mypc

Host kent-nuc
    Hostname 172.26.187.140
    User chuqi
    ProxyJump kent

